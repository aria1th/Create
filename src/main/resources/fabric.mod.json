{
  "schemaVersion": 1,
  "id": "create",
  "version": "${version}",

  "name": "Create",
  "description": "Technology that empowers the player.",
  "authors": [
    "Create Fabric Porting Team",
    "Create Team"
  ],
  "contact": {
    "issues": "https://github.com/Fabricators-of-Create/Create/issues",
    "sources": "https://github.com/Fabricators-of-Create/Create"
  },

  "license": "MIT",
  "icon": "logo.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.simibubi.create.Create"
    ],
    "client": [
      "com.simibubi.create.CreateClient"
    ],
    "fabric-datagen": [
      "com.simibubi.create.lib.data.DataInit"
    ],
    "rei": [
      "com.simibubi.create.compat.rei.CreateREI"
    ]
  },

  "mixins": [
    "create.mixins.json",
    "create_lib.mixins.common.json",
    "create_lib.mixins.client.json",
    "create_lib.mixins.compat.json"
  ],
  "accessWidener": "create.accesswidener",

  "depends": {
    "fabricloader": ">=0.13.1",
    "fabric": ">=0.46.0",
    "minecraft": ">=1.18.1",
    "registrate-fabric": "*",
    "flywheel": "*"
  }
}
